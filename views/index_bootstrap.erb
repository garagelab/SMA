            		<div class="accordion-heading">
                    	<a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion1" href="#<%=coll_obj%>">
							<!-- Apodo -->
                    		<%=elem[1][1]%>&nbsp;&nbsp;<%=elem[1][@apodo_col[:embarazo]]%>
                    	</a>
            		</div>
					<div id="<%=coll_obj%>" class="accordion-body collapse">
						<div class="accordion-inner">
							<% @requests_embarazo.each do |key, value| %>
								<% if elem[1].has_key?(key + 1) then %>
									<% if !elem[1][key + 1].empty? && !elem[1][key + 1].nil? then %> 
										<%=key + 1%>.&nbsp;<strong><%=value%></strong><br>
										<em><%=elem[1][key + 1]%></em><br>
